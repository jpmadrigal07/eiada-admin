<% include ../../partials/profile/header %>
<% include ../../partials/profile/navbar %>
<% include ../../partials/profile/sidebar %>

<div class="dashboard-ecommerce">
    <div class="container-fluid dashboard-content ">
        <!-- ============================================================== -->
        <!-- pageheader  -->
        <!-- ============================================================== -->
        <div class="row">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                <div class="page-header">
                    <h2 class="pageheader-title"><%= pageName %> </h2>
                    <div class="page-breadcrumb">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="/profile/specialist/lists"
                                        class="breadcrumb-link"><%= pageName %></a></li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- end pageheader  -->
        <!-- ============================================================== -->
        <div class="ecommerce-widget">
            <div class="row">
                <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                    <div class="card">
                        <div class="card-body">
                            <div class="input-group mb-3">
                                <input type="text" class="form-control form-control-md" id="search" placeholder="Search name, username or email..." value="<%= filter %>">
                                <div class="input-group-append">
                                    <button type="button" class="btn btn-primary" onclick="search()"><i class="fas fa-search"></i></button>
                                </div>
                            </div>
                            <div id="payment-history-list">
                                <table class="table table-bordered">
                                    <thead>
                                        <tr>
                                            <th scope="col">#</th>
                                            <th scope="col">Full Name</th>
                                            <th scope="col">Username</th>
                                            <th scope="col">Email</th>
                                            <th scope="col">Role</th>
                                            <th scope="col">Actions</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% var counter = 0; %>
                                        <% specialist.forEach(function(specialist) { %>
                                        <% counter++; %>
                                        <tr>
                                            <th scope="row"><%= counter %></th>
                                            <td><%= specialist.firstName %> <%= specialist.middleName.charAt(0) %>. <%= specialist.lastName %></td>
                                            <td><%= specialist.username %></td>
                                            <td><%= specialist.email %></td>
                                            <td><%= specialist.roles %></td>
                                            <% 
                                            var createdAt = moment(specialist.createdAt).format( 'MMM DD, YYYY hh:mm A');  
                                            var birthDate = moment(specialist.birthDate).format( 'MMM DD, YYYY');
                                            var gender = specialist.gender;  

                                            var consultationFee = "";
                                            var university = "";
                                            var associatedHospital = "";
                                            var experience = "";
                                            var nameTitle = "";
                                            var licenseNumber = "";
                                            var about = "";
                                            var languages = "";
                                            var passportURL = "";
                                            var passportID = "";  
                                            var emiratesURL = "";
                                            var emiratesID = "";  
                                            var insuranceURL = "";
                                            var insuranceID = "";  
                                            var monday = "";
                                            var mondayCount = "";
                                            var tuesday = "";
                                            var tuesdayCount = "";
                                            var wednesday = "";
                                            var wednesdayCount = "";
                                            var thursday = "";
                                            var thursdayCount = "";
                                            var friday = "";
                                            var fridayCount = "";
                                            var saturday = "";
                                            var saturdayCount = "";
                                            var sunday = "";
                                            var sundayCount = "";
                                            %>
                                            <%
                                            if(typeof specialist.specialist.info !== 'undefined') {
                                                consultationFee = specialist.specialist.info.consultationFee;
                                                university = specialist.specialist.info.university;
                                                associatedHospital = specialist.specialist.info.associatedHospital;
                                                experience = specialist.specialist.info.experience;
                                                nameTitle = specialist.specialist.info.nameTitle;
                                                licenseNumber = specialist.specialist.info.licenseNumber;
                                                about = specialist.specialist.info.about;
                                                languages = specialist.specialist.info.languages;
                                            }
                                            if(typeof specialist.identification !== 'undefined') {
                                                if(typeof specialist.identification.emirates !== 'undefined') {
                                                    emiratesURL = specialist.identification.emirates.url;
                                                    emiratesID = specialist.identification.emirates.number;  
                                                }
                                                if(typeof specialist.identification.insurance !== 'undefined') {
                                                    insuranceURL = specialist.identification.insurance.url;
                                                    insuranceID = specialist.identification.insurance.number;  
                                                }
                                                if(typeof specialist.identification.passport !== 'undefined') {
                                                    passportURL = specialist.identification.passport.url;
                                                    passportID = specialist.identification.passport.number;  
                                                }
                                            }
                                            if(typeof specialist.specialist.weeklySchedule !== 'undefined') {
                                                if(typeof specialist.specialist.weeklySchedule.monday !== 'undefined') {
                                                    monday = specialist.specialist.weeklySchedule.monday.status;
                                                    mondayCount = specialist.specialist.weeklySchedule.monday.bookingCount;
                                                }
                                                if(typeof specialist.specialist.weeklySchedule.tuesday !== 'undefined') {
                                                    tuesday = specialist.specialist.weeklySchedule.tuesday.status;
                                                    tuesdayCount = specialist.specialist.weeklySchedule.tuesday.bookingCount;
                                                }
                                                if(typeof specialist.specialist.weeklySchedule.wednesday !== 'undefined') {
                                                    wednesday = specialist.specialist.weeklySchedule.wednesday.status;
                                                    wednesdayCount = specialist.specialist.weeklySchedule.wednesday.bookingCount;
                                                }
                                                if(typeof specialist.specialist.weeklySchedule.thursday !== 'undefined') {
                                                    thursday = specialist.specialist.weeklySchedule.thursday.status;
                                                    thursdayCount = specialist.specialist.weeklySchedule.thursday.bookingCount;
                                                }
                                                if(typeof specialist.specialist.weeklySchedule.friday !== 'undefined') {
                                                    friday = specialist.specialist.weeklySchedule.friday.status;
                                                    fridayCount = specialist.specialist.weeklySchedule.friday.bookingCount;
                                                }
                                                if(typeof specialist.specialist.weeklySchedule.saturday !== 'undefined') {
                                                    saturday = specialist.specialist.weeklySchedule.saturday.status;
                                                    saturdayCount = specialist.specialist.weeklySchedule.saturday.bookingCount;
                                                }
                                                if(typeof specialist.specialist.weeklySchedule.sunday !== 'undefined') {
                                                    sunday = specialist.specialist.weeklySchedule.sunday.status;
                                                    sundayCount = specialist.specialist.weeklySchedule.sunday.bookingCount;
                                                }
                                            }
                                            %>
                                            <td><a href="#" onclick='openMoreInfo("<%= gender %>","<%= birthDate %>","<%= createdAt %>","<%= consultationFee %>", "<%= university %>", "<%= associatedHospital %>", "<%= experience %>", "<%= nameTitle %>", "<%= licenseNumber %>", "<%= about %>", "<%= languages %>")'>More Info</a> | <a href="#" onclick="openSchedule('<%= monday %>', '<%= mondayCount %>', '<%= tuesday %>',  '<%= tuesdayCount %>', '<%= wednesday %>', '<%= wednesdayCount %>', '<%= thursday %>', '<%= thursdayCount %>', '<%= friday %>', '<%= fridayCount %>',  '<%= saturday %>', '<%= saturdayCount %>', '<%= sunday %>', '<%= sundayCount %>')">Schedule</a> | <a href="/profile/specialist/expertise/<%= specialist._id %>">Expertise</a> | <a href="/profile/specialist/bookings/<%= specialist._id %>">Bookings</a> | <a href="/profile/specialist/withdrawal-requests/<%= specialist._id %>">Withdraw Requests</a> | <a href="#" onclick="openIdentification('<%= passportURL %>','<%= passportID %>','<%= emiratesURL %>','<%= emiratesID %>','<%= insuranceURL %>','<%= insuranceID %>')">View I.D's</a> | <% if(typeof specialist.approvedAt == 'undefined') { %><a href="#" onclick="openApprove('<%= specialist._id %>', 'approve')">Approve</a> <% } else { %> <a href="#" onclick="openApprove('<%= specialist._id %>', 'disapprove')">Disapprove</a> <% } %> | <a href="#" onclick="openEditspecialist('<%= specialist._id %>','<%= specialist.firstName %>','<%= specialist.middleName %>','<%= specialist.lastName %>','<%= specialist.email %>','<%= specialist.username %>','<%= specialist.password %>','<%= specialist.gender %>','<%= birthDate %>','<%= specialist.roles %>')">Edit</a> | <% if(typeof specialist.blockedAt == 'undefined') { %><a href="#" onclick="openBlock('<%= specialist._id %>', 'block')">Block</a> <% } else { %> <a href="#" onclick="openBlock('<%= specialist._id %>', 'unblock')">Unblock</a> <% } %> </td>
                                        </tr>
                                        <% }) %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

