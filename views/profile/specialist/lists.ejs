<script>
    function openIdentification(passportURL, passportID, emiratesURL, emiratesID, insuranceURL, insuranceID) {
        $('#indentification').modal('show');

        if(passportURL != "") {
            $('#passport-image').html('<img src="http://localhost:5000/uploads/'+passportURL+'" class="img-fluid" /></br></br>');
            $('#passport-id').html('Number: ' + passportID + '</br></br>');
        } else {
            $('#passport-image').html('Not set');
            $('#passport-id').html('');
        }

        if(emiratesURL != "") {
            $('#emirates-image').html('<img src="http://localhost:5000/uploads/'+emiratesURL+'" class="img-fluid" /></br></br>');
            $('#emirates-id').html('Number: ' + emiratesID + '</br></br>');
        } else {
            $('#emirates-image').html('Not set');
            $('#emirates-id').html('');
        }

        if(insuranceURL != "") {
            $('#insurance-image').html('<img src="http://localhost:5000/uploads/'+insuranceURL+'" class="img-fluid" /></br></br>');
            $('#insurance-id').html('Number: ' + insuranceID + '</br></br>');
        }else {
            $('#insurance-image').html('Not set');
            $('#insurance-id').html('');
        }

    }
    function openBlock(specialistID, status) {
        $('#block').modal('show');
        $('#delete-specialist-id').val(specialistID);
        $('#delete-specialist-status').val(status);
    }
    function openApprove(specialistID, status) {
        $('#approve').modal('show');
        $('#approve-specialist-id').val(specialistID);
        $('#approve-specialist-status').val(status);
    }
    function openMoreInfo(gender, birthDate, registrationDate, consultationFee, university, associatedHospital, experience, title, licenseNumber, about, languages) {
        $('#moreInfo').modal('show');
        $('#gender').html(gender+ "</br></br>");
        $('#birth-date').html(birthDate+ "</br></br>");
        $('#registration-date').html(registrationDate+ "</br></br>");

        if(consultationFee != "") {
            $('#consultation-fee').html(consultationFee+ "</br></br>");
            $('#university').html(university+ "</br></br>");
            $('#associated-hospital').html(associatedHospital+ "</br></br>");
            $('#experience').html(experience+ "</br></br>");
            $('#title').html(title+ "</br></br>");
            $('#license-number').html(licenseNumber+ "</br></br>");
            $('#about').html(about+ "</br></br>");
            $('#languages').html(languages+ "</br></br>");
        } else {
            $('#consultation-fee').html("Not set</br></br>");
            $('#university').html("Not set</br></br>");
            $('#associated-hospital').html("Not set</br></br>");
            $('#experience').html("Not set</br></br>");
            $('#title').html("Not set</br></br>");
            $('#license-number').html("Not set</br></br>");
            $('#about').html("Not set</br></br>");
            $('#languages').html("Not set</br></br>");
        }
    }
    function openEditspecialist(specialistID, firstName, middleName, lastName, email, username, password, gender, birthDate, roles) {
        $('#edit').modal('show');
        $('#edit-specialist-id').val(specialistID);
        $('#edit-first-name').val(firstName);
        $('#edit-middle-name').val(middleName);
        $('#edit-last-name').val(lastName);
        $('#edit-email').val(email);
        $('#edit-username').val(username);
        $('#edit-password').val(password);
        $('#edit-gender').val(gender);
        $('#datetimepicker').val(birthDate);
        $('#edit-roles').val(roles);
    }
    function openSchedule(monday, mondayCount, tuesday, tuesdayCount, wednesday, wednesdayCount, thursday, thursdayCount, friday, fridayCount, saturday, saturdayCount, sunday, sundayCount) {
        $('#schedule').modal('show');

        if(monday == "Available") {
            $('#monday').html(monday+ " and can accommodate "+mondayCount+" patient.</br></br>");
        } else if(tuesday == "Not Available") {
            $('#monday').html(monday+ ".</br></br>");
        } else {
            $('#monday').html("Not set</br></br>");
        }

        if(tuesday == "Available") {
            $('#tuesday').html(tuesday+ " and can accommodate "+tuesdayCount+" patient.</br></br>");
        } else if(tuesday == "Not Available") {
            $('#tuesday').html(tuesday+ ".</br></br>");
        } else {
            $('#tuesday').html("Not set</br></br>");
        }

        if(wednesday == "Available") {
            $('#wednesday').html(wednesday+ " and can accommodate "+wednesdayCount+" patient.</br></br>");
        } else if(wednesday == "Not Available") {
            $('#wednesday').html(wednesday+ ".</br></br>");
        } else {
            $('#wednesday').html("Not set</br></br>");
        }

        if(thursday == "Available") {
            $('#thursday').html(thursday+ " and can accommodate "+thursdayCount+" patient.</br></br>");
        } else if(thursday == "Not Available") {
            $('#thursday').html(thursday+ ".</br></br>");
        } else {
            $('#thursday').html("Not set</br></br>");
        }

        if(friday == "Available") {
            $('#friday').html(friday+ " and can accommodate "+fridayCount+" patient.</br></br>");
        } else if(friday == "Not Available") {
            $('#friday').html(friday+ ".</br></br>");
        } else {
            $('#friday').html("Not set</br></br>");
        }

        if(saturday == "Available") {
            $('#saturday').html(saturday+ " and can accommodate "+saturdayCount+" patient.</br></br>");
        } else if(saturday == "Not Available") {
            $('#saturday').html(saturday+ ".</br></br>");
        } else {
            $('#saturday').html("Not set</br></br>");
        }

        if(sunday == "Available") {
            $('#sunday').html(sunday+ " and can accommodate "+sundayCount+" patient.</br></br>");
        } else if(sunday == "Not Available") {
            $('#sunday').html(sunday+ ".</br></br>");
        } else {
            $('#sunday').html("Not set</br></br>");
        }
    }
